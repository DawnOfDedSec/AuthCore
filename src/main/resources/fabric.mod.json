{
  "schemaVersion": 1,
  "id": "authcore",
  "version": "${version}",
  "name": "AuthCore",
  "description": "AuthCore is a lightweight, server-side authentication framework for Fabric servers, enabling secure player registration and login workflows on offline-mode servers with modular, configurable, and version-safe design!",
  "authors": [
    "Ded3ec"
  ],
  "contact": {
    "homepage": "https://github.com/DawnOfDedSec/AuthCore",
    "sources": "https://github.com/DawnOfDedSec/AuthCore"
  },
  "license": "CC0-1.0",
  "icon": "assets/authCore/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.ded3ec.authcore.AuthCore"
    ],
    "fabric-datagen": [
      "net.ded3ec.authcore.AuthCoreDataGenerator"
    ]
  },
  "mixins": [
    "authcore.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "minecraft": "~1.21.11",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
  }
}